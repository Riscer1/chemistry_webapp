{% extends 'base.html.twig' %}

{% block title %}Fik ref list{% endblock %}

{% block body %}
    <h1>View fik ref</h1>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">id</th>
            <th scope="col">ref</th>
            <th scope="col">reference</th>
        </tr>
        </thead>
        <tbody>
        {% for fr in fik_ref %}
            <tr>
                <td>{{ fr.id }}</td>
                <td text="editable">{{ fr.ref }}</td>
                <td text="editable">{{ fr.reference }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
