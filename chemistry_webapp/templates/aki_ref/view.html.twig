{% extends 'base.html.twig' %}

{% block title %}Fik ref view{% endblock %}

{% block body %}
    <h1>View aki ref</h1>
    <table class="table">
        <thead>
        <tr>
            <th scope="col">ref</th>
            <th scope="col">reference</th>
        </tr>
        </thead>
        <tbody>
        {% for ar in aki_ref %}
            <tr>
                <td>{{ ar.ref }}</td>
                <td>{{ ar.reference }}</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>
{% endblock %}
